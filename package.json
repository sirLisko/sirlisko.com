{
  "name": "sirlisko.com",
  "version": "0.1.0",
  "description": "Personal Website of Luca Lischetti",
  "repository": {
    "type": "git",
    "url": "git://github.com/sirLisko/sirlisko.com.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "coverage": "jest --coverage",
    "lint": "run-p -r lint:*",
    "lint:css": "stylelint src/**/*.css",
    "lint:js": "eslint src/**/*.js",
    "postinstall": "npm run build",
    "test": "jest"
  },
  "author": {
    "name": "Luca Lischetti",
    "email": "luca@sirlisko.com",
    "url": "https://sirlisko.com"
  },
  "homepage": "http://sirlisko.com",
  "keywords": [
    "sirlisko",
    "personal",
    "website"
  ],
  "dependencies": {
    "apache-server-configs": "^3.1.0",
    "gatsby": "^2.1.27",
    "gatsby-plugin-manifest": "^2.0.22",
    "gatsby-plugin-offline": "^2.0.24",
    "gatsby-plugin-react-helmet": "^3.0.8",
    "gatsby-plugin-sass": "^2.0.10",
    "gatsby-source-filesystem": "^2.0.23",
    "gatsby-transformer-json": "^2.1.8",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "reset-css": "^4.0.1"
  },
  "devDependencies": {
    "babel-jest": "^22.4.1",
    "jest": "^22.4.2",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "prettier": "^1.16.4",
    "simulant": "^0.2.2",
    "sinon": "^7.2.7",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0"
  },
  "engines": {
    "node": ">=8"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "at-rule-empty-line-before": null,
      "rule-empty-line-before": null
    }
  },
  "jest": {
    "testURL": "http://localhost/"
  }
